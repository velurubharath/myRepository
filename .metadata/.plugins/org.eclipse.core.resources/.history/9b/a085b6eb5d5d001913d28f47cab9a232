import java.util.LinkedHashSet;

public class NoDuplicatesInArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

			LinkedHashSet array = new LinkedHashSet();
			array.add(1);
			array.add(1);
			array.add(4);
			array.add(5);
			array.add(6);
			array.add(2);
			array.add(4);
			array.add(2);
			
			System.out.println(array);
	}

}
