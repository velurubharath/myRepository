import java.util.ArrayList;
import java.util.Arrays;

public class missingNumberInSequence {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//The problem statement is to findout the missin number in the sequence where each number repeats thrice.
	//Ex: 1112233	
		//ArrayList arr = new ArrayList();
		
		String str = "11122333";
		int[] arr = str.toCharArray();
		
		System.out.println(arr[5]);
		System.out.println(arr.length);
		findMissingNumber(arr);
		
		
	}
	
	public static void findMissingNumber(char[] arr){
		
		int[] result = null;
		
		for(int i=0;i<arr.length;i++){
			System.out.println(arr[i]);
			
		switch(arr[i]){
		case (arr[i]==1):
			result[i]=(char) (result[i]+1);
			System.out.println(result[i] + i);
			
			break;
		case 2:
		case 3:
		case 4:
		case 5:
		case 6:
		case 7:
		case 8:
		case 9:
		case 0:
		 
			System.out.println("Some mistake in the input");
			
			
			
		}
			
			
		}
		
		
		
	}

}
